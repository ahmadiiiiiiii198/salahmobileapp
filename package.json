  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:admin": "tsc && vite build && npm run cap:copy",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=1000",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "start": "vite preview --host 0.0.0.0 --port $PORT",
    "test": "node console-test-script.js",
    "clean": "node -e \"const fs=require('fs');['dist','node_modules/.vite'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch(e){}})\"",
    "prebuild": "npm run optimize-images && npm run clean",
    "check-db": "node scripts/check-database.js",
    "db-status": "node scripts/check-database.js",
    "find-creds": "node scripts/find-credentials.js",
    "find-user": "node scripts/find-credentials.js",
    "optimize-images": "node scripts/optimize-images.js",
    "cap:add:android": "npx cap add android",
    "cap:copy": "npx cap copy android",
    "cap:sync": "npx cap sync android",
    "cap:open": "npx cap open android",
    "prepare:admin": "node scripts/prepare-admin-build.js",
    "android:build": "npm run build && npm run prepare:admin && npx cap sync android",
    "apk:debug": "cd android && gradlew.bat assembleDebug",
    "apk:release": "cd android && gradlew.bat assembleRelease",
    "apk:both": "cd android && gradlew.bat assembleDebug assembleRelease",
    "apk:full": "npm run android:build && cd android && gradlew.bat assembleDebug assembleRelease"
